{
  "name": "eslint-plugin-northstar",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "files": [
    "dist/**"
  ],
  "author": "Dmitrii Samsonov <dsamsonov@microsoft.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --no-splitting --minify",
    "test": "ts-node src/tests/index.ts",
    "format": "prettier \"src/**/*.{ts,tsx,json}\" \"*.{js,json}\" \"!.pnp.js\" --list-different",
    "format:fix": "yarn format --write"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/estree": "^0.0.50",
    "@types/node": "^16.4.13",
    "@typescript-eslint/parser": "^4.29.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.0",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.29.1",
    "eslint": "^7.32.0"
  }
}
